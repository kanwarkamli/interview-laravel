includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    paths:
        - app/
        - tests/

    # Level 9 is the highest level
    level: 5

    ignoreErrors:
        - '#Call to an undefined method [a-zA-Z0-9\\_]#'
        - '#Access to an undefined property [a-zA-Z0-9\\_]#'
        - '#Undefined variable: \$this#'
